<script>
//проверка входных параметров при регистрации страницы
function check_value()
{
	
	var str='';
	var count=document.addForm.count_url.value;
	var price=document.addForm.url_price.value;
	if(count.replace(/[^0-9]/g,'')!=count || count=='')
	  str+='<br><li>Count';
	if(price.replace(/[^0-9.,]/g,'')!=price || price=='')
	  str+='<br><li>Price';
	if(str!='')
	{
		document.getElementById('error_message').innerHTML='Вы не заполнили следующие поля::'+str;
		return false;
	}
	return true;
}

//проверка
function check_input_value()
{
	var error_init=0;
	
	//Проверка заполнения полей
	var str="Вы не заполнили следующие поля::<br>";	
	if(document.loginForm.e_mail.value=="") {str+="<li>E-mail<br>";error_init=1;}
	if(document.loginForm.password.value=="") {str+="<li>Пароль<br>";error_init=1;}
	
	//Вывод ошибки
	if(error_init)
	{
		document.getElementById('error_message').innerHTML=str;
		return false;
	}
	//Проверке корректности данных
	error_init=0;
	str="Следующие поля заполнены некорректно:<br>";
	if(!/[0-9a-z_-]+@[0-9a-z_-]+.[a-z]{2,4}/i.test(document.loginForm.e_mail.value)) 
		{str+="<li>E-mail<br>";error_init=1;}
	if(document.loginForm.password.value.replace(/[^a-zA-Z0-9%,.!?=&$\(\)\*\+; \-_:\/~\[\]]/g,"")!=document.loginForm.password.value || document.loginForm.password.value.length<6)
		{str+="<li>Пароль";error_init=1;}

	if(error_init)
	{
		document.getElementById('error_message').innerHTML=str;
		return false;
	}
	return true;
}

