<script>

function set_menu_opt()
{
	document.cookie='select_menu=1';
	document.cookie='selected_submenu=1';
}

function set_menu_web()
{
	document.cookie='select_menu=2';
	document.cookie='selected_submenu=1';
}

function set_menu_seo()
{
	document.cookie='select_menu=3';
	document.cookie='selected_submenu=1';
}

//удаление куков (выход пользователя)

function setcookie(user_name)
{
	var tmp_date=new Date();
	tmp_date.setDate(tmp_date.getDate()+30);
	document.cookie='user_name='+user_name+';expires='+tmp_date.toString();
}

function delcookie()
{
	var tmp_date=new Date()-10;
	document.cookie="user_name=;expires=Thu, 01-Jan-70 00:00:01 GMT;";
	document.cookie="e_mail=;expires=Thu, 01-Jan-70 00:00:01 GMT;";
	document.cookie="passsword=;expires=Thu, 01-Jan-70 00:00:01 GMT;";
	window.location.href="/?action=exit";

}
//проверка
function check_input_value_head()
{return true;
	var error_init=0;
	
	//Проверка заполнения полей
	var str="Вы не заполнили следующие поля::<br>";	
	if(document.loginFormHead.e_mail.value=="") {error_e_mail=1;error_init=1;}
	if(document.loginFormHead.password.value=="") {error_password=1;error_init=1;}
	
	//Вывод ошибки
	if(error_init)
	{
		//document.getElementById('error_message').innerHTML=str;
		return false;
	}
	//Проверке корректности данных
	error_init=0;
	str="Следующие поля заполнены некорректно:<br>";
	if(!/^[0-9a-z._\-]+@[0-9a-z._\-]+.[a-z]{2,4}$/i.test(document.loginFormHead.e_mail.value)) 
		{error_e_mail=1;;error_init=1;}
	if(document.loginFormHead.password.value.replace(/[^a-zA-Z0-9%,.!?=&$\(\)\*\+; \-_:\/~\[\]]/g,"")!=document.loginFormHead.password.value || document.loginForm.password.value.length<6)
		{str+="<li>Пароль";error_init=1;}

	if(error_init)
	{
		//document.getElementById('error_message').innerHTML=str;
		return false;
	}
	return false;
}
function loginFormHead_submit(){
	var login_e_mail=$('#form1 #login_e_mail').val();
	var login_password=$('#form1 #login_password').val();
	var login_remember=($('#form1 #login_remember').attr('checked')==true ? 'on' : '');
	var captcha=$('#form1 #captcha_code').val();
	var post_data={'e_mail':login_e_mail, 'password': login_password, 'remember': login_remember };
	if (typeof captcha !== "undefined"){
		post_data['keystring']=captcha;
	}
	$('#formlogin_error').load('/login.php', post_data);
}
function t_change_logo(obj){
	var standart_logo='/img/logos/logo_net.png';
	var holiday_logo='/img/logos/';
	var get=standart_logo;
	if(holiday_logo=='/img/logos/') return;
	$('.change_logo').toggle();
	
	if(obj.id=='standart_logo'){
		get=holiday_logo;
                $.post("/template/get_img_logo.php",{'hol_logos_off' : 0});
	}else{
            $.post("/template/get_img_logo.php",{'hol_logos_off' : 1});
	}
	$('#header_left_logo').css('backgroundImage', 'url('+get+')');
}

